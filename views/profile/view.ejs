<%- include('../partials/header')%>
<div class="container">
    <h1 class="card-title">Profile</h1>
    <hr>
    <div class="col-md-12">
    <form action="" method="get">
        <div class="form-group row">
            <label for="checkEmail" class="col-sm-2 col-form-label">
                <input class="form-check-input" name="checkEmail" type="checkbox" id="email">
                Email
            </label>
            <div class="col-sm-10">
                <input type="text" name="email" class="form-control" id="email" placeholder="Email">
            </div>
        </div>

        <div class="form-group row">
            <label for="name" class="col-sm-2 col-form-label">
                <input class="form-check-input" name="checkName" type="checkbox" id="name">
                Pasword
            </label>
            <div class="col-sm-10">
                <input type="text" name="name" class="form-control" id="name" placeholder="Name">
            </div>
        </div>

        <div class="form-group row">
            <label for="member" class="col-sm-2 col-form-label">
                <input class="form-check-input" name="checkMember" type="checkbox" id="member">
                Member
            </label>
            <div class="col-sm-10">
                <input type="text" name="member" class="form-control" id="member" placeholder="Member">
            </div>
        </div>
    </form>

    <div class="form-group row">
        <div class="col-sm-10">
          <button type="submit" class="btn btn-dark" value="clicked">Search</button>

          <a href="/">
            <button type="submit" class="btn btn-dark">reset</button>
          </a>
          
        </div>
      </div>
</div>
</div>

<%- include('../partials/footer')%>