<%- include('../partials/header')%>
<div class="container">
    <h1 class="card-title">Add Your New Project !</h1>
    <hr>
    <div class="col-md-12 b">
        <form method="POST">
            <div class="form-group row">
                <label for="projectName" class="col-sm-2 col-form-label">Project Name</label>
                <div class="col-sm-10">
                    <input type="text" name="projectName" class="form-control" id="projectName" placeholder="Project Name">
                </div>
            </div>

            <div class="row b">
                <legend class="col-form-label col-sm-2 pt-0">Members</legend>
                <div class="col-sm-10">
                    <% data.forEach((item, index) => {%>
                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" name="members" id="members<%= index + 1%>" value="<%= item.userid%>">
                        <label class="form-check-label" for="members<%= index + 1%>">
                          <%= item.fullname %>
                        </label>
                    </div>
                    <%})%>
                </div>
            </div>

            <div class="form-group row">
                <div class="col-sm-10">
                    <button type="submit" class="btn btn-outline-primary" value="clicked">Save</button>
            </div>

        </form>
    </div>
</div>
<%- include('../partials/footer')%>