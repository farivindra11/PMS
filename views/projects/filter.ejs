<div class="container">
    <h1 class="card-title">Projects</h1>
    <hr>
    <h3 class="card-title">Filters</h3>
    <div class="col-md-12">
    <form action="" method="get">
        <div class="form-group row">
            <label for="id" class="col-sm-2 col-form-label">
                <input class="form-check-input" name="checkId" type="checkbox" id="id">
                ID
            </label>
            <div class="col-sm-10">
                <input type="number" name="id" class="form-control" id="id" placeholder="ID">
            </div>
        </div>

        <div class="form-group row">
            <label for="name" class="col-sm-2 col-form-label">
                <input class="form-check-input" name="checkName" type="checkbox" id="name">
                Name
            </label>
            <div class="col-sm-10">
                <input type="text" name="name" class="form-control" id="name" placeholder="Name">
            </div>
        </div>

        <div class="form-group row">
            <label for="member" class="col-sm-2 col-form-label">
                <input class="form-check-input" name="checkMember" type="checkbox" id="member">
                Member
            </label>
            <div class="col-sm-10">
                <input type="text" name="member" class="form-control" id="member" placeholder="Member">
            </div>
        </div>
    </form>

    <div class="form-group row">
        <div class="col-sm-10">
          <button type="submit" class="btn btn-dark" value="clicked">Search</button>

          <a href="/">
            <button type="submit" class="btn btn-dark">reset</button>
          </a>
          
        </div>
      </div>
</div>
</div>
